<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.hq.mapper.AccountMapper">
    <resultMap type="cn.com.hq.entity.Account" id="accountResultMap">
        <id property="accountid" column="accountid"/>
        <result property="name" column="name"/>
        <result property="passWord" column="password"/>
        <result property="age" javaType="int" jdbcType="INTEGER"  column="age"/>
        <result property="address"  jdbcType="VARCHAR" column="address"/>
        <result property="email"  jdbcType="VARCHAR" column="email"/>
        <result property="phone"  jdbcType="VARCHAR" column="phone"/>
        <result property="description"  jdbcType="VARCHAR" column="description"/>
    </resultMap>
</mapper>